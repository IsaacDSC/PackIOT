<div id="slider" >
{{#each lines}}

    {{#if dataValues.link}}   
    <iframe id="teste" class="selected" time="{{dataValues.time}}" src="{{dataValues.link}}"></iframe>
    {{/if}}
    {{#if dataValues.video}}    
    <video id="teste" time="{{dataValues.time}}"  muted autoplay controls="controls" >
        <source src="/video/uploads/{{dataValues.video}}" type="video/mp4">
    </video>
    {{/if}}
    {{#if dataValues.image}}      
        <img id="teste" src="/images/uploads/{{dataValues.image}}" time="{{dataValues.time}}" class="img-fluid" alt="{{dataValues.image}}">    
    {{/if}}

{{/each}}
</div>
